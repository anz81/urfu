@model Urfu.Its.Web.Models.EditVariantSelectionGroupRowViewModel

@Html.HiddenFor(model => model.Id)
<tr>
    <td>
        @Html.EditorFor(model => model.Name, new { htmlAttributes = new { @class = "form-control" } })
    </td>
    <td>
        @Html.EditorFor(model => model.TestUnits, new { htmlAttributes = new { @class = "form-control" } })
    </td>
    <td>
        @Html.EditorFor(model => model.SelectionDeadline, new { @class = "form-control datecontrol", type = "date" })
    </td>

    <td>
        @Html.ActionLink("Удалить", "Delete", new { id = Model.Id })
    </td>
</tr>

